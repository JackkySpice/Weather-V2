{
  "name": "codex-cloud-wrapper",
  "version": "1.0.0",
  "description": "Web chat UI wrapper around OpenAI Codex CLI with cloud transcript storage and Render deployment",
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:web",
    "build:web": "vite build --config web/vite.config.js",
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node src/index.js",
    "dev:web": "vite --config web/vite.config.js --host",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@openai/codex": "^0.39.0",
    "@vscode/ripgrep": "file:vendor/vscode-ripgrep",
    "ansi-to-html": "^0.7.0",
    "compression": "^1.7.4",
    "express": "^5.1.0",
    "node-pty": "^1.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve-static": "^1.15.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.1.4",
    "vite": "^5.4.8"
  },
  "overrides": {
    "@vscode/ripgrep": "file:vendor/vscode-ripgrep"
  }
}
